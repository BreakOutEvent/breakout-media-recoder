{
  "posthook": {},
  "mediafolder": "./media/",
  "s3": {
    "accessKey": "access-key",
    "secretAccessKey": "secret-key",
    "bucketName": "bucket-name",
    "region": "region-code"
  },
  "types": [
    {
      "typename": "image",
      "sizes": [
        {
          "name": "thumbnail",
          "side": 80,
          "quality": 0.8
        },
        {
          "name": "small",
          "side": 200,
          "quality": 0.65
        },
        {
          "name": "medium",
          "side": 600,
          "quality": 0.8
        },
        {
          "name": "large",
          "side": 1200,
          "quality": 0.8
        }
      ]
    },
    {
      "typename": "video",
      "sizes": [
        {
          "name": "low",
          "side": 480,
          "videobitrate": 1200,
          "audiobitrate": 92
        },
        {
          "name": "medium",
          "side": 720,
          "videobitrate": 2200,
          "audiobitrate": 128
        },
        {
          "name": "hd",
          "side": 1280,
          "videobitrate": 4200,
          "audiobitrate": 160
        },
        {
          "name": "fullhd",
          "side": 1920,
          "videobitrate": 6200,
          "audiobitrate": 160
        }
      ]
    },
    {
      "typename": "audio",
      "sizes": [
        {
          "name": "low",
          "bitrate": 48
        },
        {
          "name": "medium",
          "bitrate": 92
        },
        {
          "name": "high",
          "bitrate": 192
        }
      ]
    }
  ]
}